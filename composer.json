{
    "name": "chrishemmings/electio-client",
    "description": "A PHP client for the Electio delivery management system",
    "keywords": [
        "Electio"
        "delivery",
        "courier"
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/chrishemmings/electio-client",
    "license": "Apache v2",
    "authors": [
        {
            "name": "Chris Hemmings",
            "email": "chris@hemmin.gs"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "ChrisHemmings\\ElectioClient\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "ChrisHemmings\\ElectioClient\\" : "test/" }
    }
}
